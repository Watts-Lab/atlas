{
  "name": "atlas",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc.json",
    "lint": "tsc && eslint ./src/",
    "lint:fix": "eslint --fix ./src/",
